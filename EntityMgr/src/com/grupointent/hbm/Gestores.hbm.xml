<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class table="dbo.Tbl_GestoresCorrespondencia" entity-name="Gestores">
		<id length="50" type="string" column="Usuario" name="Usuario">
		</id>
		<property length="100" type="string" column="Nombre" name="Nombre"
			not-null="false" />
		<property length="200" type="string" column="Cargo" name="Cargo"
			not-null="false" />
		<property length="200" type="string" column="Dependencia"
			name="Dependencia" />
		<property length="10" type="string" column="GestorPrincipal"
			name="Gestor principal" not-null="false" />
	</class>
</hibernate-mapping>